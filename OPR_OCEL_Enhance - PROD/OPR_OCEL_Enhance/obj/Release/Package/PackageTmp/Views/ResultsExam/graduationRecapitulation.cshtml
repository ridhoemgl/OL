@{
    ViewBag.Title = "Report | Graduation Recapitulation";
    Layout = "~/Views/Shared/_LayoutMaster.cshtml";
    ViewBag.dashboard = "Report";
    ViewBag.dashboardSmall = "Graduation Recapitulation";
}
<input type="hidden" id="urlPath" name="urlPath" value="@ViewBag.baseurl" />
<script type="text/x-kendo-template" id="averagePercent">
    <label id="average"></label>
</script>
<div class="col-md-12">
    <h5>Graduation Recapitulation</h5>
    <hr style="width:100%;" />
    @*<div class="form-horizontal">*@
        <div class="row">
            <div class="col-md-5">
                <div class="form-group">
                    <label>Bulan</label>
                    <select class="form-control" id="bulan">
                        <option value="">-- Bulan --</option>
                        <option value="1">Januari</option>
                        <option value="2">Februari</option>
                        <option value="3">Maret</option>
                        <option value="4">April</option>
                        <option value="5">Mei</option>
                        <option value="6">Juni</option>
                        <option value="7">Juli</option>
                        <option value="8">Agustus</option>
                        <option value="9">September</option>
                        <option value="10">Oktober</option>
                        <option value="11">November</option>
                        <option value="12">Desember</option>
                    </select>
                </div>
            </div>
            <div class="col-md-4">
                <div class="form-group">
                    <label>Tahun</label>
                    <select id="tahun" class="form-control" style="height:80%;">
                        <option value="">-- Pilih Tahun --</option>
                    </select>
                </div>
            </div>
            <div class="col-md-3">
                <div class="form-group">
                     <br />
                    <button class="btn btn-primary btn-sm btn-block" id="btn_cari" onclick="Cari()" style="margin-top:3%;"><i class="fa fa-search"></i> Cari</button>
                </div>
            </div>
          
        </div>
    @*</div>*@
    <hr style="width:100%;" />
    <div class="col-md-12 ">
        @*<h5>Graduation Recapitulation</h5>*@
     
        <div id="grid"></div>
    </div>
</div>
<script type="text/javascript" src="@ViewBag.pathParent/Js/graduationRecapitulation.js"></script>

