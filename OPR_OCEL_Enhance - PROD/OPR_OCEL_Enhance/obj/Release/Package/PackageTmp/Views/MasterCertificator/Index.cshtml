@{
    ViewBag.Title = "Master Sertifikator";
    Layout = "~/Views/Shared/_LayoutMaster.cshtml";
    ViewBag.dashboard = "Master";
    ViewBag.dashboardSmall = "Sertifikator";
    ViewBag.pathParent = Url.Content("~").Substring(0, Url.Content("~").Length - 1);
}
<input type="hidden" id="urlPath" name="urlPath" value="@ViewBag.pathParent" />


<div id="wnd_addCertificator">

    <div class="row">
        <div class="col-md-3">
            <label class="control-label pull-right" for="id_nrp">NRP : </label>
        </div>
        <div class="col-md-4">
            <input disabled type="text" id="id_nrp" name="id_nrp" class="k-textbox" style="background-color:#c6dbda; width:200px;" />
        </div>
        <div class="col-md-3">
            <input type="button" class="k-button primary" value="..." onclick="openEmployee()" />
        </div>
    </div>
    <br />

    <div class="row">
        <div class="col-md-3">
            <label class="control-label pull-right" for="id_nrp">Nama : </label>
        </div>
        <div class="col-md-4">
            <input disabled type="text" id="id_nama" name="id_nrp" class="k-textbox" style="background-color:#c6dbda; width:200px;" />
        </div>
    </div>
    <br />
    <div class="row">
        <div class="col-md-3">
            <label class="control-label pull-right" for="id_nrp">Distrik : </label>
        </div>
        <div class="col-md-4">
            <input disabled type="text" id="id_distrik" name="id_distrik" class="k-textbox" style="background-color:#c6dbda; width:200px;" />
        </div>
    </div>
    <br />
    <div class="row">
        <div class="col-md-3">
            <label class="control-label pull-right" for="id_nrp">Departemen : </label>
        </div>
        <div class="col-md-4">
            <input disabled type="text" id="id_departemen" name="id_departemen" class="k-textbox" style="background-color:#c6dbda; width:200px;" />
        </div>
    </div>
    <br />
    <div class="row">
        <div class="col-md-3">
            <label class="control-label pull-right" for="id_nrp">Posisi : </label>
        </div>
        <div class="col-md-4">
            <input disabled type="text" id="id_posisi" name="id_posisi" class="k-textbox" style="background-color:#c6dbda; width:200px;" />
        </div>
    </div>
    <br />
    <div class="row">
        <div class="col-md-3">
            <label class="control-label pull-right" for="id_date">Tanggal Kadaluarsa : </label>
        </div>
        <div class="col-md-4">
            <input id="datepicker" title="datepicker" style="width:200px" />
        </div>
    </div>
    <br />
    <div class="row">
        <div class="col-md-3">
            <label class="control-label pull-right" for="id_status">Status : </label>
        </div>
        <div class="col-md-4">
            <input type="checkbox" name="id_status" id="id_status" />
        </div>
    </div>
    <hr />
    <div class="row">
        <div class="col-md-3">
            <label class="control-label pull-right" for="id_isAsesor">Is Asesor : </label>
        </div>
        <div class="col-md-4">
            <input type="checkbox" name="id_isAsesor" id="id_isAsesor" />
        </div>
    </div>
    <br/>
    <div class="row">
            <div class="col-md-3">
                <label class="control-label pull-right" for="id_nrp">No. Registrasi :</label>
            </div>
            <div class="col-md-4">
                <input type="text" id="id_noRegis" name="id_noRegis" class="k-textbox" style="width:200px;" />
            </div>
    </div>
    <br/>
    <div class="row" style="justify-content: center;">
        <button id="btn_save" class="k-button" style="width:85px; margin:10px;"><span class="glyphicon glyphicon-floppy-disk"></span>&nbsp;&nbsp;Save</button>
        <button id="btn_cancel" class="k-button" style="width: 85px; margin: 10px;"><span class="glyphicon glyphicon-remove"></span>&nbsp;&nbsp;Cancel</button>
    </div>
</div>


<div id="wnd_employee">
    <div id="gridEmployee"></div>
</div>

<div id="wnd_vendor">
    <div id="gridVendor"></div>
</div>

<div id="gridCertificator"></div>

<script type="text/javascript" src="@ViewBag.pathParent/Js/MasterCertificator.js"></script>