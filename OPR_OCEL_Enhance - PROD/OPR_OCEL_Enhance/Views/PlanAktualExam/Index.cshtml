
@{
    ViewBag.Title = "Report Monitoring plan dan Aktual";
    Layout = "~/Views/Shared/_LayoutMaster.cshtml";
    ViewBag.dashboard = "Report";
    ViewBag.dashboardSmall = "Monitoring Plan dan Aktual";
}
<input type="hidden" id="urlPath" name="urlPath" value="@ViewBag.base_url" />
<div class="row">
    <div class="col-md-1">
        <label>Posisi</label>
    </div>
    <div class="col-md-2">
        <input type="text" id="txtPosisi" style="width:150px;" />
    </div>

    <div class="col-md-1">
        <label>Jenis Ujian</label>
    </div>
    <div class="col-md-2">
        <input type="text" id="txtJenisUjian" style="width:150px; background-color:#d5ebed" />
    </div>

    <div class="col-md-1">
        <button type="button" id="btn_cari">
            <i class="fa fa-search" aria-hidden="true"></i>
            Cari
        </button>
    </div>
</div>
<hr />
<div class="row">
    <div class="col-lg-12">
        <div id="grid_data">

        </div>
    </div>
</div>
<script type="text/javascript" src="@ViewBag.base_url/ContentLayout/js/jquery.toast.js"></script>
<script type="text/javascript" src="@ViewBag.base_url/js/MonitoringPlanAktualUjian.js"></script>

